{
  "kind": "collectionType",
  "collectionName": "courses",
  "info": {
    "singularName": "course",
    "pluralName": "courses",
    "displayName": "Course"
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "title": {
      "type": "string",
      "required": true
    },
    "slug": {
      "type": "uid",
      "targetField": "title",
      "required": true
    },
    "short_description": {
      "type": "text"
    },
    "description": {
      "type": "blocks",
      "required": false
    },
    "instructors": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::intructor.intructor"
    },
    "modules": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::module.module"
    }
  }
}
